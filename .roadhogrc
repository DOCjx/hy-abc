{
    "entry": "src/index.js",
    "disableCSSModules": false,
    "theme": "./src/theme.js",
    "env": {
        "development": {
            "extraBabelPlugins": [
                "dva-hmr",
                "transform-runtime",
                ["import", { "libraryName": "antd", "style": true }]
            ]
        },
        "production": {
            "extraBabelPlugins": [
                "transform-runtime",
                ["import", { "libraryName": "antd", "style": true}]
            ]
        }
    }
}
